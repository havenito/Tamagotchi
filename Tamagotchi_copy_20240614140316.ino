void gestionBoutons() {
    static bool selectButtonWasPressed = false; // Variable pour suivre l'état du bouton de sélection
    static unsigned long selectButtonPressedStartTime = 0; // Temps auquel le bouton de sélection a été enfoncé

    // Vérifier l'état du bouton de sélection
    if (digitalRead(BTN_SELECT_PIN) == LOW && !selectButtonWasPressed) {
        // Si le bouton de sélection est enfoncé, enregistrer le temps auquel il a été enfoncé
        selectButtonPressedStartTime = millis();
        selectButtonWasPressed = true;
    } else if (digitalRead(BTN_SELECT_PIN) == HIGH && selectButtonWasPressed) {
        // Si le bouton de sélection est relâché et qu'il était auparavant enfoncé, déterminer la durée pendant laquelle il a été enfoncé
        unsigned long buttonPressDuration = millis() - selectButtonPressedStartTime;
        selectButtonWasPressed = false;

        // Vérifier si le bouton de sélection a été maintenu enfoncé pendant plus de 2 secondes
        if (buttonPressDuration >= 2000) {
            // Retourner au menu principal
            selectedIconIndex = 0; // Sélectionner le premier icône du menu principal
            afficherIcones(); // Afficher à nouveau les icônes
            display.display(); // Rafraîchir l'écran
            return;
        }
    }

    // Vérifier l'état du bouton gauche
    if (digitalRead(BTN_LEFT_PIN) == LOW) {
        // Déplacer la sélection vers la gauche
        selectedIconIndex = (selectedIconIndex - 1 + 10) % 10; // 10 est le nombre total d'icônes
        display.display(); // Mettre à jour l'affichage
        delay(200); // Délai pour éviter les rebonds
    }

    // Vérifier l'état du bouton droit
    if (digitalRead(BTN_RIGHT_PIN) == LOW) {
        // Déplacer la sélection vers la droite
        selectedIconIndex = (selectedIconIndex + 1) % 10; // 10 est le nombre total d'icônes
        display.display(); // Mettre à jour l'affichage
        delay(200); // Délai pour éviter les rebonds
    }

    // Si le bouton de sélection est maintenu enfoncé, exécuter l'action correspondante
    if (digitalRead(BTN_SELECT_PIN) == LOW) {
        switch(selectedIconIndex) {
            case 0:
                actionHP();
                break;
            case 1:
                actionForme();
                break;
            case 2:
                actionFaim();
                break;
            case 3:
                actionSettings();
                break;
            case 4:
                actionJeux();
                break;
            case 5:
                actionDouche();
                break;
            case 6:
                actionDodo();
                break;
            case 7:
                actionManger();
                break;
            case 8:
                actionSoins();
                break;
            case 9:
                actionToilettes();
                break;
        }
    }
}

