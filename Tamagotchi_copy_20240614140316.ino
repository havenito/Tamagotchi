bool menuFaimInProgress = false; // Déclaration unique de la variable menuFaimInProgress

void actionManger() {

if (niveauFaim >= 90) {
    // Afficher un message indiquant que le chat a déjà assez mangé
    display.clearDisplay();
    display.setTextSize(1);
    display.setTextColor(WHITE);

    // Message à afficher
    const char* message = " a deja assez mange";

    // Calculer la position horizontale pour centrer le texte
    int textWidth = strlen(catName) * 6 + strlen(message) * 6;  // Taille totale du texte en pixels (caractères * taille de police)
    int xPos = (128 - textWidth) / 2;  // Calculer la position horizontale centrée
    int yPos = (64 - 8) / 2;  // Calculer la position verticale centrée

    display.setCursor(xPos, yPos);  // Positionner le curseur au centre
    display.print(catName);
    display.print(message);
    display.display();
    delay(2000); // Afficher le message pendant 2 secondes
    return; // Retourner immédiatement
}

    afficherEcranManger();
    delay(100);

    while (true) {
        if (digitalRead(BTN_LEFT_PIN) == LOW && !menuFaimInProgress && !buttonPressed) {
            selectedIconIndex = (selectedIconIndex - 1 + 6) % 6;
            afficherEcranManger();
            delay(200);  // Délai pour éviter les rebonds
            buttonPressed = true;  // Bouton pressé
            lastButtonPressTime = millis();  // Enregistrer le temps du dernier appui sur le bouton
        }

        if (digitalRead(BTN_RIGHT_PIN) == LOW && !menuFaimInProgress && !buttonPressed) {
            selectedIconIndex = (selectedIconIndex + 1) % 6;
            afficherEcranManger();
            delay(200);  // Délai pour éviter les rebonds
            buttonPressed = true;  // Bouton pressé
            lastButtonPressTime = millis();  // Enregistrer le temps du dernier appui sur le bouton
        }

        if (digitalRead(BTN_SELECT_PIN) == LOW) {
            if (!menuFaimInProgress && !buttonPressed) {
                switch (selectedIconIndex) {
                    case 0:
                        actionPourcentage();
                        break;
                    case 1:
                        actionPoisson();
                        break;
                    case 2:
                        actionPate();
                        break;
                    case 3:
                        actionLait();
                        break;
                    case 4:
                        selectedIconIndex = 0;
                        afficherIcones();
                        return;
                    case 5:
                        actionSettings();
                        break;
                }
            } else if (menuFaimInProgress && millis() - lastButtonPressTime > 2000) {  // Si le bouton est pressé pendant 2 secondes
                selectedIconIndex = 4;  // Sélectionne l'icône Retour
                afficherEcranManger();
                menuFaimInProgress = false;  // Fin de l'action
                delay(500);  // Délai pour éviter les rebonds
            }
        }
        
        // Réinitialiser buttonPressed après un certain délai
        if (millis() - lastButtonPressTime > 200) {
            buttonPressed = false;
        }
    }
}

