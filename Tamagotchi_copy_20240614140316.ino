unsigned long lastButtonPressTime = 0;
bool buttonPressed = false;
bool menuInProgress = false; // Déclaration unique de la variable menuInProgress

void actionJeux() {
    afficherEcranJeux();
    delay(200);

    while (true) {
        if (digitalRead(BTN_LEFT_PIN) == LOW && !menuInProgress && !buttonPressed) {
            selectedIconIndex = (selectedIconIndex - 1 + 5) % 5;
            afficherEcranJeux();
            delay(200);  // Délai pour éviter les rebonds
            buttonPressed = true;  // Bouton pressé
            lastButtonPressTime = millis();  // Enregistrer le temps du dernier appui sur le bouton
        }

        if (digitalRead(BTN_RIGHT_PIN) == LOW && !menuInProgress && !buttonPressed) {
            selectedIconIndex = (selectedIconIndex + 1) % 5;
            afficherEcranJeux();
            delay(200);  // Délai pour éviter les rebonds
            buttonPressed = true;  // Bouton pressé
            lastButtonPressTime = millis();  // Enregistrer le temps du dernier appui sur le bouton
        }

        if (digitalRead(BTN_SELECT_PIN) == LOW) {
            if (!menuInProgress && !buttonPressed) {
                switch (selectedIconIndex) {
                    case 0:
                        actionQuizz();
                        break;
                    case 1:
                        actionProut();
                        break;
                    case 2:
                        actionClick();
                        break;
                    case 3:
                        selectedIconIndex = 0;
                        afficherIcones();
                        return;
                    case 4:
                        actionSettings();
                        break;
                }
            } else if (menuInProgress && millis() - lastButtonPressTime > 2000) {  // Si le bouton est pressé pendant 2 secondes
                selectedIconIndex = 3;  // Sélectionne l'icône Retour
                afficherEcranJeux();
                menuInProgress = false;  // Fin de l'action
                delay(500);  // Délai pour éviter les rebonds
            }
        }
        
        // Réinitialiser buttonPressed après un certain délai
        if (millis() - lastButtonPressTime > 200) {
            buttonPressed = false;
        }
    }
}

void afficherEcranJeux() {
    display.clearDisplay();  // Clear display to remove previous highlights

    // Draw chat
    display.drawBitmap(0, 15, Chat[0], 128, 33, WHITE);

    // Draw icons
    display.drawBitmap(0, 54, Quizz[0], 10, 10, WHITE);  // Quizz icon
    display.drawBitmap(20, 54, Prout[0], 10, 10, WHITE);  // Prout icon
    display.drawBitmap(40, 54, Click[0], 10, 10, WHITE);  //Click icon
    display.drawBitmap(0, 0, Retour[0], 10, 10, WHITE);  // Retour icon
    display.drawBitmap(118, 0, Settings[0], 10, 10, WHITE);  // Settings icon
    display.drawBitmap(0, 50, barre[0], 128, 1, WHITE); 
    // Highlight selected icon
    switch (selectedIconIndex) {
        case 0:
            display.drawRect(0, 54, 10, 10, WHITE);  // Quizz
            break;
        case 1:
            display.drawRect(20, 54, 10, 10, WHITE);  // Prout
            break;
        case 2:
            display.drawRect(40, 54, 10, 10, WHITE);  // Prout
            break;
        case 3:
            display.drawRect(0, 0, 10, 10, WHITE);  // Retour
            break;
        case 4:
            display.drawRect(118, 0, 10, 10, WHITE);  // Settings
            break;
    }

    display.display();
}

