//Verification état soin 
void verifierSante() {
    if (millis() - dernierTempsMaladie >= intervalleMaladie) {
        dernierTempsMaladie = millis();
        if (etatSante == EN_BONNE_SANTE) {
            etatSante = BIENTOT_MALADE;
        } else if (etatSante == BIENTOT_MALADE) {
            etatSante = MALADE;
        }
    }
}

// Fonction pour dessiner une barre de progression à une position donnée
void drawProgressBar(int x, int y, int percentage) {
    int width = 128 - x; // Calculer la largeur de la barre en fonction de la position x et de la largeur de l'écran (128 pixels)
    int height = 8; // Hauteur fixe de la barre de progression
    int progress = (width * percentage) / 100; // Calculer la largeur de la partie remplie de la barre en fonction du pourcentage

    // Dessiner le contour de la barre vide
    display.drawRect(x, y, width, height, WHITE);

    // Dessiner la partie remplie de la barre
    display.fillRect(x, y, progress, height, WHITE);
}

