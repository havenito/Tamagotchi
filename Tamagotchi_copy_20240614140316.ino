// Constantes pour les joueurs et les cases vides
#define PLAYER_X 'X'
#define PLAYER_O 'O'
#define EMPTY ' '

// Tableau pour le plateau de jeu
char board[3][3];
int cursorX = 0, cursorY = 0;  // Positions du curseur
bool isPlayerTurn = true;  // Indicateur de tour du joueur

// Fonction pour initialiser le plateau de jeu
void initializeBoard() {
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            board[i][j] = EMPTY;
        }
    }
}

// Fonction pour afficher le plateau de jeu
void displayBoard() {
    display.clearDisplay();
    display.setTextSize(2);
    display.setTextColor(WHITE);

    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            display.setCursor(j * 20 + 10, i * 20 + 10);
            display.write(board[i][j]);
        }
    }

    // Dessiner les lignes du plateau
    display.drawLine(30, 10, 30, 70, WHITE);
    display.drawLine(50, 10, 50, 70, WHITE); 
    display.drawLine(10, 30, 70, 30, WHITE); 
    display.drawLine(10, 50, 70, 50, WHITE); 

    // Dessiner le curseur
    display.drawRect(cursorX * 20 + 9, cursorY * 20 + 9, 18, 18, WHITE);
    display.display();
}

// Fonction pour vérifier si le plateau est plein
bool isBoardFull() {
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (board[i][j] == EMPTY) {
                return false;
            }
        }
    }
    return true;
}

// Fonction pour vérifier le gagnant
char checkWinner() {
    for (int i = 0; i < 3; i++) {
        if (board[i][0] == board[i][1] && board[i][1] == board[i][2] && board[i][0] != EMPTY) {
            return board[i][0];
        }
        if (board[0][i] == board[1][i] && board[1][i] == board[2][i] && board[0][i] != EMPTY) {
            return board[0][i];
        }
    }

    // Vérification des diagonales
    if (board[0][0] == board[1][1] && board[1][1] == board[2][2] && board[0][0] != EMPTY) {
        return board[0][0];
    }
    if (board[0][2] == board[1][1] && board[1][1] == board[2][0] && board[0][2] != EMPTY) {
        return board[0][2];
    }

    return EMPTY;  // Retourne EMPTY si pas de gagnant
}

// Fonction pour le mouvement de l'IA
void aiMove() {
    // Vérifie si l'IA peut gagner
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (board[i][j] == EMPTY) {
                board[i][j] = PLAYER_O;
                if (checkWinner() == PLAYER_O) {
                    return;
                }
                board[i][j] = EMPTY;
            }
        }
    }
    // Vérifie si l'IA peut bloquer le joueur
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++) {
            if (board[i][j] == EMPTY) {
                board[i][j] = PLAYER_X;
                if (checkWinner() == PLAYER_X) {
                    board[i][j] = PLAYER_O;
                    return;
                }
                board[i][j] = EMPTY;
            }
        }
    }

    // Jouer au centre si possible
    if (board[1][1] == EMPTY) {
        board[1][1] = PLAYER_O;
        return;
    }

    // Jouer dans les coins
    int corners[4][2] = { {0, 0}, {0, 2}, {2, 0}, {2, 2} };
    for (int k = 0; k < 4; k++) {
        int i = corners[k][0];
        int j = corners[k][1];
        if (board[i][j] == EMPTY) {
            board[i][j] = PLAYER_O;
            return;
        }
    }

    // Jouer sur les côtés
    int edges[4][2] = { {0, 1}, {1, 0}, {1, 2}, {2, 1} };
    for (int k = 0; k < 4; k++) {
        int i = edges[k][0];
        int j = edges[k][1];
        if (board[i][j] == EMPTY) {
            board[i][j] = PLAYER_O;
            return;
        }
    }
}

void displayCenteredText(const char* text) {
    int16_t x, y;
    uint16_t width, height;

    // Définir la police à la taille 1
    display.setTextSize(1);
    display.setTextColor(WHITE);

    // Obtenir la largeur et la hauteur du texte
    display.getTextBounds(text, 0, 0, &x, &y, &width, &height);

    // Calculer les coordonnées pour centrer le texte
    x = (nombreDePixelsEnLargeur - width) / 2;
    y = (nombreDePixelsEnHauteur - height) / 2;

    // Afficher le texte centré
    display.clearDisplay();
    display.setCursor(x, y);
    display.println(text);
    display.display();
    delay(2000);
}

void actionProut() {
    initializeBoard();  // Initialise le plateau de jeu
    displayBoard();  // Affiche le plateau de jeu
    delay(2000);

    while (true) {
        displayBoard();

        // Déplacement du curseur
        if (digitalRead(BTN_LEFT_PIN) == LOW) {
            cursorX--;
            if (cursorX < 0) {
                cursorX = 2;
                cursorY--;
                if (cursorY < 0) {
                    cursorY = 2;
                }
            }
            delay(200); 
        }
        if (digitalRead(BTN_RIGHT_PIN) == LOW) {
            cursorX++;
            if (cursorX > 2) {
                cursorX = 0;
                cursorY++;
                if (cursorY > 2) {
                    cursorY = 0;
                }
            }
            delay(200);
        }
        if (digitalRead(BTN_SELECT_PIN) == LOW) {
            // Placer le coup du joueur
            if (board[cursorY][cursorX] == EMPTY) {
                board[cursorY][cursorX] = PLAYER_X;
                displayBoard();
                delay(200); 

                // Vérifie si le joueur a gagné
                if (checkWinner() == PLAYER_X) {
                    displayCenteredText("Joueur gagne!");
                    return;
                }
                // Vérifie si le plateau est plein
                if (isBoardFull()) {
                    displayCenteredText("Egalite!");
                    return;
                }

                // Mouvement de l'IA
                aiMove();
                displayBoard();

                // Vérifie si l'IA a gagné
                if (checkWinner() == PLAYER_O) {
                    displayCenteredText("IA gagne!");
                    return;
                }
                // Vérifie si le plateau est plein
                if (isBoardFull()) {
                    displayCenteredText("Egalite!");
                    return;
                }
            }
        }
    }
}


// Variables globales pour le jeu
int notePosition = 0;      // Position verticale des notes
int noteColumn = 0;        // Colonne actuelle des notes
int score = 0;             // Score du joueur
int gameSpeed = 50;        // Vitesse du jeu, modifiée par le niveau de difficulté
int bestScore = 0;

void readBestScoreFromEEPROM() {
    EEPROM.begin(2);  // Initialiser la mémoire EEPROM
    // Lire le meilleur score depuis la mémoire EEPROM
    bestScore = EEPROM.read(0) << 8 | EEPROM.read(1);
    if (bestScore == 65535) {
        // Si la valeur lue est 65535 (valeur par défaut non valide), initialiser à 0
        bestScore = 0;
    }
    EEPROM.end();  // Terminer l'utilisation de la mémoire EEPROM
}

void writeBestScoreToEEPROM(int newBestScore) {
    EEPROM.begin(2);  // Initialiser la mémoire EEPROM
    EEPROM.write(0, (newBestScore >> 8) & 0xFF);  // Écrire l'octet de poids fort
    EEPROM.write(1, newBestScore & 0xFF);         // Écrire l'octet de poids faible
    EEPROM.commit();  // Enregistrer les modifications dans la mémoire EEPROM
    EEPROM.end();     // Terminer l'utilisation de la mémoire EEPROM
}

void actionClick() {
    // Initialisation de l'écran OLED
    if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) { 
        Serial.println(F("SSD1306 allocation failed"));
        for (;;);
    }
    display.display();
    delay(2000);
    display.clearDisplay();

    // Lire le meilleur score depuis la mémoire EEPROM
    readBestScoreFromEEPROM();

    // Afficher le menu principal avec le meilleur score
    display.setTextSize(2);
    display.setTextColor(WHITE);
    display.setCursor(0, 0);
    display.println("Niveaux :");
    display.setTextSize(1);
    display.setCursor(0, 25);
    display.println("Left: Ez");
    display.setCursor(0, 35);
    display.println("Right: Moyen");
    display.setCursor(0, 45);
    display.println("Select: Dur!");
    display.setCursor(0, 55);
    display.println("Best: " + String(bestScore));
    display.display();

    // Attente de la sélection du niveau
    while (true) {
        if (digitalRead(BTN_LEFT_PIN) == LOW) {
            gameSpeed = 100;  // Niveau facile
            break;
        }
        if (digitalRead(BTN_RIGHT_PIN) == LOW) {
            gameSpeed = 50;   // Niveau moyen
            break;
        }
        if (digitalRead(BTN_SELECT_PIN) == LOW) {
            gameSpeed = 25;   // Niveau difficile
            break;
        }
        delay(100);
    }

    // Démarrage du jeu après la sélection du niveau
    display.clearDisplay();
    display.setTextSize(1);
    display.setTextColor(WHITE);
    score = 0;
    notePosition = 0;
    noteColumn = random(0, 3);
    
    while (true) {
        display.clearDisplay();

        // Dessiner une note descendante
        display.setTextSize(1);
        display.setTextColor(WHITE);
        display.setCursor(0, 0);
        display.println("Score: " + String(score));

        // Dessiner la zone de clic
        display.fillRect(0, 50, nombreDePixelsEnLargeur, 10, WHITE);

        // Position de l'indicateur de note
        int indicatorWidth = nombreDePixelsEnLargeur / 3;
        display.fillRect(noteColumn * indicatorWidth, notePosition, indicatorWidth, 10, WHITE);

        // Déplacement de la note vers le bas
        notePosition += 2;
        if (notePosition > nombreDePixelsEnHauteur) {
            // Si la note dépasse l'écran, game over
            gameOver();
            return;
        }

        // Détection de la pression des boutons
        if (notePosition >= 50 && notePosition <= 60) {
            if (digitalRead(BTN_LEFT_PIN) == LOW) {
                if (noteColumn == 0) {
                    score++;
                    notePosition = 0;
                    noteColumn = random(0, 3);
                } else {
                    gameOver();
                    return;
                }
            }
            if (digitalRead(BTN_RIGHT_PIN) == LOW) {
                if (noteColumn == 1) {
                    score++;
                    notePosition = 0;
                    noteColumn = random(0, 3);
                } else {
                    gameOver();
                    return;
                }
            }
            if (digitalRead(BTN_SELECT_PIN) == LOW) {
                if (noteColumn == 2) {
                    score++;
                    notePosition = 0;
                    noteColumn = random(0, 3);
                } else {
                    gameOver();
                    return;
                }
            }
        }

        display.display();
        delay(gameSpeed);
    }
}

void gameOver() {
    // Vérifier et mettre à jour le meilleur score
    if (score > bestScore) {
        bestScore = score;
        // Écrire le nouveau meilleur score dans la mémoire EEPROM
        writeBestScoreToEEPROM(bestScore);
    }

    display.clearDisplay();
    display.setTextSize(2);
    display.setTextColor(WHITE);
    display.setCursor(0, 0);
    display.println("Game Over");
    display.setTextSize(1);
    display.setCursor(0, 25);
    display.println("Score: " + String(score));
    display.setCursor(0, 35);
    display.println("Best: " + String(bestScore));
    display.setCursor(0, 45);
    display.println("Left: Retry");
    display.setCursor(0, 55);
    display.println("Right: Quit");
    display.display();

    // Attente de la sélection de l'utilisateur
    while (true) {
        if (digitalRead(BTN_LEFT_PIN) == LOW) {
            // Rejouer
            actionClick();
            return;
        }
        if (digitalRead(BTN_RIGHT_PIN) == LOW) {
            // Retourner à l'écran de sélection de niveau
            display.clearDisplay();
            return;
        }
        delay(100);
    }
}


