void actionToilettes() {
  if (niveauHygiene >= 90) {
    // Afficher un message indiquant que le chat a déjà assez mangé
    display.clearDisplay();
    display.setTextSize(1);
    display.setTextColor(WHITE);

    // Message à afficher
    const char* message = " n'a rien a faire";

    // Calculer la position horizontale pour centrer le texte
    int textWidth = strlen(catName) * 6 + strlen(message) * 6;  // Taille totale du texte en pixels (caractères * taille de police)
    int xPos = (128 - textWidth) / 2;  // Calculer la position horizontale centrée
    int yPos = (64 - 8) / 2;  // Calculer la position verticale centrée

    display.setCursor(xPos, yPos);  // Positionner le curseur au centre
    display.print(catName);
    display.print(message);
    display.display();
    delay(2000); // Afficher le message pendant 2 secondes
    return; // Retourner immédiatement
}
unsigned long animationStartTime = millis();
unsigned long frameDuration = 1000; // Durée de chaque image en millisecondes
int frameIndex = 0;

    while (millis() - animationStartTime < frameDuration * 6) {
        display.clearDisplay(); // Effacer l'écran avant d'afficher chaque nouvelle image
        display.drawBitmap(0, 0, action_toilettes[frameIndex], 128, 64, WHITE);
        display.display();

        // Passer à l'image suivante toutes les frameDuration millisecondes
        if (millis() - animationStartTime >= frameDuration * (frameIndex + 1)) {
            frameIndex++;
        }

        // S'assurer que frameIndex reste dans les limites
        frameIndex %= 6;

        delay(10); // Court délai pour éviter une utilisation excessive du processeur
    }
    niveauJoie += 10;
    niveauJoie = constrain(niveauJoie, 0, 100);
    niveauHygiene += 10;
    niveauHygiene = constrain(niveauHygiene, 0, 100);
    actionStartTime = millis(); // Enregistrer le temps de début de l'action
    actionInProgress = true; // Début de l'action
}






