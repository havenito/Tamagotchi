void mettreAJourFatigue() {
    unsigned long currentTime = millis();
    unsigned long fatigueTimeElapsed = currentTime - lastFatigueUpdateTime; // Calculer le temps écoulé depuis la dernière mise à jour du niveau de fatigue

    // Mettre à jour la fatigue toutes les 15 secondes
    if (fatigueTimeElapsed >= 60000) {
        niveauFatigue -= 1; // Décrémenter le niveau de fatigue de 1%
        lastFatigueUpdateTime = currentTime; // Mettre à jour le temps de la dernière mise à jour
        
        // Sauvegarder le niveau de fatigue
        preferences.putInt("fatigue", niveauFatigue);

        // Vérifier les seuils pour la fatigue
        verifierSeuil(niveauFatigue, "Fatigue");
    }

    // Si le chat est en train de dormir, il récupère 1% toutes les 10 secondes
    if (actionInProgress && selectedIconIndex == 6 && fatigueTimeElapsed >= 10000) {
        niveauFatigue += 1; // Incrémenter le niveau de fatigue de 1%
        lastFatigueUpdateTime = currentTime; // Mettre à jour le temps de la dernière mise à jour
        
        // Sauvegarder le niveau de fatigue
        preferences.putInt("fatigue", niveauFatigue);
    }

    // S'assurer que le niveau de fatigue reste dans les limites (0-100)
    niveauFatigue = constrain(niveauFatigue, 0, 100);
}

