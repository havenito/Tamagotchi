void actionPate() {
    display.clearDisplay();
    unsigned long animationStartTime = millis();
    unsigned long frameDuration = 1000; // Durée de chaque image en millisecondes
    int frameIndex = 0;

    // Animation de 9 frames
    while (millis() - animationStartTime < frameDuration * 9) { // Afficher chaque image pendant 9 secondes
        display.clearDisplay(); // Effacer l'écran avant d'afficher chaque nouvelle image
        display.drawBitmap(0, 0, action_pate[frameIndex], 128, 64, WHITE);
        display.display();

        // Passer à l'image suivante toutes les frameDuration millisecondes
        if (millis() - animationStartTime >= frameDuration * (frameIndex + 1)) {
            frameIndex++;
        }

        // S'assurer que frameIndex reste dans les limites
        frameIndex %= 9;

        delay(10); // Court délai pour éviter une utilisation excessive du processeur
    }

    // Mettre à jour les niveaux de joie et de faim
    niveauJoie += 5;
    niveauJoie = constrain(niveauJoie, 0, 100);
    niveauFaim += 15;
    niveauFaim = constrain(niveauFaim, 0, 100);
    menuFaimInProgress = millis(); // Enregistrer le temps de début de l'action
    actionInProgress = true; // Début de l'action
}

