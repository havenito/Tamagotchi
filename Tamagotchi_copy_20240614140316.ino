// Fonction pour gérer une question donnée
void handleQuestion(int questionIndex, const char* questions[][3], int correctAnswers[]) {
    display.clearDisplay();  // Efface l'écran
    display.setTextSize(1);  // Définit la taille du texte
    display.setTextColor(WHITE);  // Définit la couleur du texte
    display.setCursor(0, 0);  // Positionne le curseur en haut à gauche
    display.println(questions[questionIndex][0]);  // Affiche la question
    display.setCursor(0, 20);  // Positionne le curseur plus bas
    display.println("1. " + String(questions[questionIndex][1]));  // Affiche la première réponse
    display.setCursor(0, 30);  // Positionne le curseur encore plus bas
    display.println("2. " + String(questions[questionIndex][2]));  // Affiche la deuxième réponse
    display.display();  // Affiche le tout à l'écran

    // Boucle infinie pour attendre la réponse de l'utilisateur
    while (true) {
        // Vérifie si le bouton de gauche est pressé
        if (digitalRead(BTN_LEFT_PIN) == LOW) {
            // Vérifie si la première réponse est correcte
            if (correctAnswers[questionIndex] == 1) {
                display.clearDisplay();
                display.setCursor(0, 0);
                display.println("Bonne reponse !");
                // Augmente les niveaux de joie et de forme
                niveauJoie += 10;
                niveauJoie = constrain(niveauJoie, 0, 100);
                niveauForme += 20;
                niveauForme = constrain(niveauForme, 0, 100);
            } else {
                display.clearDisplay();
                display.setCursor(0, 0);
                display.println("Mauvaise reponse !");
                // Diminue les niveaux de joie et de forme
                niveauJoie -= 5;
                niveauJoie = constrain(niveauJoie, 0, 100);
                niveauForme -= 10;
                niveauForme = constrain(niveauForme, 0, 100);
            }
            display.display();
            delay(1000);  // Attente de 1 seconde
            break;  // Sort de la boucle
        }
        // Vérifie si le bouton de droite est pressé
        if (digitalRead(BTN_RIGHT_PIN) == LOW) {
            // Vérifie si la deuxième réponse est correcte
            if (correctAnswers[questionIndex] == 2) {
                display.clearDisplay();
                display.setCursor(0, 0);
                display.println("Bonne reponse !");
                // Augmente les niveaux de joie et de forme
                niveauJoie += 10;
                niveauJoie = constrain(niveauJoie, 0, 100);
                niveauForme += 20;
                niveauForme = constrain(niveauForme, 0, 100);
            } else {
                display.clearDisplay();
                display.setCursor(0, 0);
                display.println("Mauvaise reponse !");
                // Diminue les niveaux de joie et de forme
                niveauJoie -= 5;
                niveauJoie = constrain(niveauJoie, 0, 100);
                niveauForme -= 10;
                niveauForme = constrain(niveauForme, 0, 100);
            }
            display.display();
            delay(1000);  // Attente de 1 seconde
            break;  // Sort de la boucle
        }
    }
}

