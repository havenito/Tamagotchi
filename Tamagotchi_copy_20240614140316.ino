    display.clearDisplay();
    unsigned long animationStartTime = millis();
    unsigned long frameDuration = 1000; // Durée de chaque image en millisecondes
    unsigned long lastFatigueUpdateTime = millis(); // Pour suivre le dernier temps de mise à jour de la fatigue
    int frameIndex = 0;

    // Répéter les images 4 et 5 tant que l'utilisateur ne retourne pas au menu
    bool repeatFrames = true;

    while (repeatFrames || (millis() - animationStartTime < 6000)) { // Afficher les images pendant 6 secondes ou jusqu'à ce que l'utilisateur retourne au menu
        unsigned long currentTime = millis();

