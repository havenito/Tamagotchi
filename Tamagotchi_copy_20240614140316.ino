bool selectButtonDisabled = false;
unsigned long selectButtonDisableTime = 0;

// Durée de désactivation temporaire du bouton de sélection (en millisecondes)
const unsigned long SELECT_BUTTON_DISABLE_DURATION = 500; // 0.5 seconde

// Variables pour la gestion du bouton de sélection
bool selectButtonEnabled = true; // Indique si le bouton de sélection est actuellement activé
unsigned long lastSelectPressTime = 0; // Temps du dernier appui sur le bouton de sélection

// Durée d'attente après un appui sur le bouton de sélection (en millisecondes)
const unsigned long SELECT_BUTTON_DELAY = 500; // 0.5 seconde

String currentMessage = "";
bool messageDisplayed = false;
unsigned long messageStartTime = 0;

void loop() {
    unsigned long currentTime = millis(); // Obtenir le temps actuel en millisecondes

    if (enteringName) {
        displayNameInputScreen(); // Afficher l'écran de saisie du nom
        handleNameInput(); // Gérer la saisie du nom
    } else {
        // Mettre à jour les niveaux de fatigue, joie, forme, hygiène et faim
        mettreAJourFatigue();
        mettreAJourJoie();
        mettreAJourForme();
        mettreAJourHygiene();
        mettreAJourFaim();
        verifierSante();

        if (actionInProgress) {
            // Si une action est en cours et que le bouton de sélection est maintenu enfoncé pendant plus de 2 secondes
            if (digitalRead(BTN_SELECT_PIN) == LOW && currentTime - actionStartTime >= 2000) {
                selectedIconIndex = 0; // Réinitialiser l'index de l'icône sélectionnée
                afficherIcones(); // Afficher les icônes
                display.display(); // Mettre à jour l'affichage
                actionInProgress = false; // Indiquer que l'action est terminée
                lastSelectPressTime = currentTime; // Mettre à jour le temps du dernier appui sur le bouton de sélection
                selectButtonEnabled = false; // Désactiver temporairement le bouton de sélection
            }
        } else {
            // Si le bouton de sélection est temporairement désactivé et que le délai est écoulé, réactiver le bouton
            if (!selectButtonEnabled && currentTime - lastSelectPressTime >= SELECT_BUTTON_DELAY) {
                selectButtonEnabled = true;
            }

            // Si l'animation de bienvenue n'a pas encore commencé et que la durée d'affichage de bienvenue est écoulée
            if (!animationStarted && (currentTime - lastDisplayTime >= welcomeDisplayDuration)) {
                animationStarted = true; // Indiquer que l'animation a commencé
                lastFrameTime = currentTime; // Initialiser le temps du dernier cadre d'animation
                frameIndex = 0; // Réinitialiser l'index du cadre d'animation
            }

            // Si l'animation a commencé, mettre à jour l'affichage de l'animation
            if (animationStarted) {
                if (currentTime - lastFrameTime >= frameDelay) {
                    display.clearDisplay(); // Effacer l'affichage
                    display.drawBitmap(0, 15, anime_chat[frameIndex], 128, 33, WHITE); // Dessiner le cadre d'animation actuel
                    afficherIcones(); // Afficher les icônes
                    display.display(); // Mettre à jour l'affichage
                    frameIndex = (frameIndex + 1) % 8; // Passer au cadre d'animation suivant
                    lastFrameTime = currentTime; // Mettre à jour le temps du dernier cadre d'animation
                }
            }

            // Gérer les boutons si le bouton de sélection est activé
            if (selectButtonEnabled) {
                gestionBoutons();
            }
        }
    }
}

// Afficher l'écran de saisie du nom
void displayNameInputScreen() {
    display.clearDisplay(); // Effacer l'affichage
    display.setTextSize(1); // Définir la taille du texte à 1
    display.setCursor(0, 0); // Positionner le curseur en haut à gauche
    display.print("Nom du chat: "); // Afficher le texte "Nom du chat: "
    display.print(catName); // Afficher le nom actuel du chat
    display.setCursor(0, 16); // Positionner le curseur pour le clavier

    // Afficher le clavier
    for (int row = 0; row < 4; row++) {
        for (int col = 0; col < 10; col++) {
            if (row == selectedRow && col == selectedCol) {
                display.setTextColor(BLACK, WHITE); // Texte inversé pour la sélection
            } else {
                display.setTextColor(WHITE); // Texte blanc normal
            }
            display.print(keyboard[row][col]); // Afficher le caractère du clavier
            display.print(" "); // Ajouter un espace entre les caractères
        }
        display.println(); // Passer à la ligne suivante du clavier
    }
    display.display(); // Mettre à jour l'affichage
}

// Gérer la saisie du nom
void handleNameInput() {
    // Vérifier si le bouton de sélection est enfoncé
    if (digitalRead(BTN_SELECT_PIN) == LOW) {
        char selectedChar = keyboard[selectedRow][selectedCol]; // Obtenir le caractère sélectionné sur le clavier
        if (selectedChar == '<') {
            // Si le caractère sélectionné est '<', supprimer le dernier caractère du nom
            if (charIndex > 0) {
                catName[--charIndex] = '\0'; // Décrémenter charIndex et ajouter une fin de chaîne '\0'
            }
        } else if (selectedChar == '>' || selectedChar == 'E') {
            // Si le caractère sélectionné est '>' ou 'E', terminer la saisie du nom
            enteringName = false; // Indiquer que la saisie du nom est terminée
        } else {
            // Ajouter le caractère sélectionné au nom si la longueur est inférieure à 19
            if (charIndex < 19) {
                catName[charIndex++] = selectedChar; // Ajouter le caractère et incrémenter charIndex
                catName[charIndex] = '\0'; // Ajouter une fin de chaîne '\0'
            }
        }
        delay(200); // Délai pour éviter les rebonds
    }

    // Gérer les déplacements du curseur sur le clavier (gauche/droite)
    if (digitalRead(BTN_LEFT_PIN) == LOW) {
        selectedCol--; // Déplacer le curseur vers la gauche
        if (selectedCol < 0) {
            selectedCol = 9; // Revenir à la dernière colonne de la ligne précédente
            selectedRow--; // Déplacer le curseur vers la ligne précédente
            if (selectedRow < 0) {
                selectedRow = 3; // Revenir à la dernière ligne si on dépasse le haut du clavier
            }
        }
        delay(200); // Délai pour éviter les rebonds
    }

    if (digitalRead(BTN_RIGHT_PIN) == LOW) {
        selectedCol++; // Déplacer le curseur vers la droite
        if (selectedCol > 9) {
            selectedCol = 0; // Revenir à la première colonne de la ligne suivante
            selectedRow++; // Déplacer le curseur vers la ligne suivante
            if (selectedRow > 3) {
                selectedRow = 0; // Revenir à la première ligne si on dépasse le bas du clavier
            }
        }
        delay(200); // Délai pour éviter les rebonds
    }
}

